#include<stdio.h>
void main()
{
    int n,a[30],i,j,c=0,k=0,b[30],d=0,min;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            c++;
            b[k]=a[i];
            k++;
            d++;
        }
        else
           c=0;
    }
    b[k]='\0';
    for(k=0;k<d;k++)
    printf("%d",b[k]);
    min=b[0];
    for(i=0;i<n;i++)
    {
    if(b[i]>min)
    min=b[i];
    }
    printf("\n%d",min);
}
